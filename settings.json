{
  "workbench.startupEditor": "newUntitledFile",
  "workbench.editor.enablePreview": false,
  "workbench.colorTheme": "Themer Dark",
  "workbench.iconTheme": "material-icon-theme",
  "files.autoSave": "onWindowChange",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "explorer.confirmDragAndDrop": false,
  "window.newWindowDimensions": "inherit",
  "files.exclude": {
    "**/.idea": true
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.suggestSelection": "first",
  "workbench.colorCustomizations": {
    "titleBar.activeBackground": "#121A21",
    "titleBar.activeForeground": "#41B883",
    "activityBar.background": "#121A21",
    "activityBar.foreground": "#41B883",
    "tab.activeBorderTop": "#41B883",
    "tab.inactiveBackground": "#121A21",
    "sideBar.background": "#1f262c",
    "sideBarSectionHeader.foreground": "#41B883",
    "sideBarSectionHeader.background": "#121A21",
    "breadcrumb.foreground": "#3cb880",
    "editorLineNumber.foreground": "#3aa273",
    "editorLineNumber.activeForeground": "#0ded88",
    "activityBarBadge.background": "#41B883",
    "activityBarBadge.foreground": "#121A21",
    "statusBar.background": "#121A21",
    "statusBar.foreground": "#41B883",
    "statusBar.border": "#121A217f",
    "editorError.foreground": "#c50a0a00",
    "editorError.border": "#c50a0a",
    "editorWarning.border": "#aeb10c00",
    "editorWarning.foreground": "#aeb10c",
    "editorBracketMatch.border": "#41B883",
    "editorBracketHighlight.foreground1": "#5391aeec",
    "editorBracketHighlight.foreground2": "#85B464ec",
    "editorBracketHighlight.foreground3": "#E2D269ec",
    "editorBracketHighlight.foreground4": "#EA915Eec",
    "editorBracketHighlight.foreground5": "#BC677Bec",
    "editorBracketHighlight.foreground6": "#7A4E8Aec"
